---
apiVersion: apiserver.config.k8s.io/v1
kind: AdmissionConfiguration
plugins:
  - name: PodSecurity
    configuration:
      apiVersion: pod-security.admission.config.k8s.io/v1
      kind: PodSecurityConfiguration
      defaults:
        enforce: "{{ k8s_pod_security_default_enforce }}"
        enforce-version: "latest"
        audit: "{{ k8s_pod_security_default_audit }}"
        audit-version: "latest"
        warn: "{{ k8s_pod_security_default_warn }}"
        warn-version: "latest"
      exemptions:
        usernames: []
        runtimeClasses: []
        namespaces:
          - kube-system
