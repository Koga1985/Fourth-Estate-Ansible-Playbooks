# HashiCorp Vault Environment Variables
# Generated by Ansible - DO NOT EDIT MANUALLY

VAULT_ADDR={{ vault_api_addr }}
VAULT_CACERT={{ vault_tls_ca_file }}
VAULT_SKIP_VERIFY=false
{% if vault_http_proxy %}
http_proxy={{ vault_http_proxy }}
{% endif %}
{% if vault_https_proxy %}
https_proxy={{ vault_https_proxy }}
{% endif %}
{% if vault_no_proxy %}
no_proxy={{ vault_no_proxy }}
{% endif %}
{% if vault_enterprise and vault_install_license %}
VAULT_LICENSE_PATH={{ vault_config_path }}/vault.hclic
{% endif %}
{% if vault_enable_fips %}
VAULT_FIPS_MODE=1
{% endif %}
